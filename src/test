Sub GetHyperlinkFromWorksheet()
    Dim ws As Worksheet
    Dim hl As Hyperlink
    Dim rng As Range
    
    Set ws = ActiveSheet
    Set rng = Selection
    
    ' シート内のすべてのハイパーリンクを検索
    For Each hl In ws.Hyperlinks
        If hl.Range.Address = rng.Address Then
            Debug.Print "リンク先URL: " & hl.Address
            Exit Sub
        End If
    Next
    
    Debug.Print "このセルにはハイパーリンクがありません。"
End Sub
