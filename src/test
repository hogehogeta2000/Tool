Sub GetHyperlinkFromCell()
    Dim ws As Worksheet
    Dim rng As Range
    Dim hyperlinkURL As String
    
    Set ws = ActiveSheet
    Set rng = Selection ' 選択されたセル
    
    ' 選択セルにハイパーリンクがあるか確認
    If rng.Hyperlinks.Count > 0 Then
        hyperlinkURL = rng.Hyperlinks(1).Address
        Debug.Print "リンク先URL: " & hyperlinkURL
    Else
        Debug.Print "このセルにはハイパーリンクがありません。"
    End If
End Sub
